package firstprogram;

public class CollegeDetails {
public static void main(String[] args) {
	System.out.println("My name is Teja Prakash Chinthoju");
	System.out.println("Iam a B-tech student");
	System.out.println("In sree vahini institute of Science and technology");
}
}

