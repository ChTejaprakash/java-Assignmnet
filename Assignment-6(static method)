//using static method to perform operations
//Source Code


package Assignment6_staticmethods;
public class StaticMethods {
public static void main(String[] args) {
	operators();
}
public static void operators() {
	int a=10;
	int b=20;
	System.out.println(a==b);
	System.out.println(a!=b);
	System.out.println(a>b);
	System.out.println(a<b);
	System.out.println(a+b);
	System.out.println(a-b);
	System.out.println(a*b);
	System.out.println(a/b);
	System.out.println(a|b);
	System.out.println(a&b);
	
}
}

//Output

false
true
false
true
30
-10
200
0
30
0
