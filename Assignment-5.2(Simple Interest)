//Finding Simple Interest
//Source Code

package assignment5;
public class SimpleInterest {
int p=100000;
int t=2;
int r=2;
int i=p*t*r/100;
public static void main(String[] args) {
	SimpleInterest si=new SimpleInterest();
	System.out.println("simple interest="+si.i);
}
}

//Output

simple interest=4000
