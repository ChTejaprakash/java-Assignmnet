// Local Variables
#Source Code

package variables;
public class Variable {
public static void main(String[] args) {
	int a=10;
	float b=20.0f;
	double c=20.3;
	char d='T';
	long e=12345987l;
	short f=1478;
	byte g =1;
	System.out.println(a);
	System.out.println(b);
	System.out.println(c);
	System.out.println(d);
	System.out.println(e);
	System.out.println(f);
	System.out.println(g);
}
}


#Output
10
20.0
20.3
T
12345987
1478
1
